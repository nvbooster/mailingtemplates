{
	"type" : "project",
	"license" : "MIT",
	"require" : {
		"php" : "^7.1.3",
		"ext-iconv" : "*",
		"sensio/framework-extra-bundle" : "^5.1",
		"symfony/asset" : "^4.0",
		"symfony/console" : "^4.0",
		"symfony/flex" : "^1.0",
		"symfony/framework-bundle" : "^4.0",
		"symfony/lts" : "^4@dev",
		"symfony/templating" : "^4.0",
		"symfony/twig-bundle" : "^4.0",
		"symfony/webpack-encore-pack" : "^1.0",
		"symfony/yaml" : "^4.0",
		"tijsverkoyen/css-to-inline-styles" : "^2.2"
	},
	"require-dev" : {
		"symfony/dotenv" : "^4.0"
	},
	"config" : {
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"replace" : {
		"symfony/polyfill-iconv" : "*",
		"symfony/polyfill-php71" : "*",
		"symfony/polyfill-php70" : "*",
		"symfony/polyfill-php56" : "*"
	},
	"conflict" : {
		"symfony/symfony" : "*"
	},
	"extra" : {
		"symfony" : {
			"allow-contrib" : false
		}
	},
	"name" : "nvbooster/mailingtemplates",
	"description" : "Tool for assisting in email templates developing",
	"homepage" : "https://github.com/nvbooster/mailingtemplates",
	"authors" : [{
			"name" : "nvb",
			"email" : "nvb@aproxima.ru",
			"homepage" : "https://github.com/nvbooster"
		}
	],
	"keywords" : [
		"email",
		"templates"
	]
}